{
	"name": "react-ts-vite-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port 3000",
		"build": "tsc && vite build",
		"build:app": "tsc && vite build --mode production",
		"build:dev": "tsc && vite build --mode development",
		"preview": "vite preview",
		"husky-prepare": "husky install",
		"test": "vitest -c ./vite.config.ts --run",
		"pre-push-test": "vitest --run",
		"lint": "eslint ./src --ext .jsx,.ts,.tsx",
		"type-check": "tsc",
		"pretty": "prettier --write src/**/*.{ts,tsx,css}"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.0.5",
		"@tanstack/react-query-devtools": "^5.0.5",
		"axios": "^1.5.1",
		"i18next": "^23.6.0",
		"mobx": "^6.10.2",
		"mobx-react-lite": "^4.0.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.3.1",
		"react-router-dom": "^6.17.0",
		"styled-components": "^6.1.0",
		"vite-tsconfig-paths": "^4.2.1"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^14.0.0",
		"@types/jest": "^29.5.6",
		"@types/react": "^18.2.31",
		"@types/react-dom": "^18.2.14",
		"@types/styled-components": "^5.1.29",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"@vitejs/plugin-react": "^4.1.0",
		"eslint": "^8.52.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^1.0.1",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	}
}
